#include <stdio.h>
#include<locale.h>
#include<math.h>
int main(){
   int f;
    printf("Выберите одну из 7 программ: ");
    scanf("%d",&f);
    switch(f) {
        case 1:
            printf("Выбрано 1 задание\n");
            int a1, b1;
            printf("Введите число А: ");
            scanf("%d", &a1);
            printf("Введите число B: ");
            scanf("%d", &b1);
            if(a1>2 && b1<=3)
                printf("Высказывание «Справедливы неравенства A > 2 и B ≤ 3» истинно\n");
                else
                    printf("Высказывание «Справедливы неравенства A > 2 и B ≤ 3» ложно\n");
        break;
        case 2:
            printf("Выбрано 2 задание\n");
            int a2, b2, c2;
            printf("Введите число А: ");
            scanf("%d", &a2);
            printf("Введите число B: ");
            scanf("%d", &b2);
            printf("Введите число C: ");
            scanf("%d", &c2);
            if(a2<b2 && b2<c2)
                printf("Высказывание «Справедливо двойное неравенство A < B < C» истинно\n");
                else
                    printf("Высказывание «Справедливо двойное неравенство A < B < C» ложно\n");
            break;
        case 3:
            printf("Выбрано 3 задание\n");
            int a3;
            printf("Введите число А: ");
            scanf("%d",&a3);
            if(a3>9 && a3<100 && a3%2==0)
                printf("Высказывание «Данное число является четным двузначным» истинно\n");
                else
                    printf("Высказывание «Данное число является четным двузначным» ложно\n");
            break;
        case 4:
            printf("Выбрано 4 задание\n");
            int a4, b, c;
            printf("Введите число А: ");
            scanf("%d",&a4);
            b=a4/100;
            a4=a4-b*100;
            c=a4/10;
            a4=a4-c*10;
            if((a4>c && c>b) || (b>c && c>a4))
                printf("Высказывание «Цифры данного числа образуют возрастающую или убывающую последовательность». истинно\n");
                else
                    printf("Высказывание «Цифры данного числа образуют возрастающую или убывающую последовательность». ложно\n");
            break;
        case 5:
            printf("Выбрано 5 задание\n");
            int a5, b5, c5, d;
            printf("Введите число А: ");
            scanf("%d",&a5);
            b5=a5/1000;
            a5=a5-b5*1000;
            c5=a5/100;
            a5=a5-c5*100;
            d=a5/10;
            a5=a5-d*10;
            if(b5==a5 && c5==d)
                printf("Высказывание «Данное число читается одинаково слева направо и справа налево истинно\n");
                else
                    printf("Высказывание «Данное число читается одинаково слева направо и справа налево ложно\n");
            break;
        case 6:
            printf("Выбрано 6 задание\n");
            int a6, b6, c6;
            printf("Введите сторону треугольника А: ");
            scanf("%d",&a6);
            printf("Введите сторону треугольника B: ");
            scanf("%d",&b6);
            printf("Введите сторону треугольника C: ");
            scanf("%d",&c6);
            if(((c6*c6)+(b6*b6)==(a6*a6)) || ((a6*a6)+(b6*b6)==(c6*c6)) || ((a6*a6)+(c6*c6)==(b6*b6)))
                printf("Высказывание «Треугольник со сторонами a, b, c является прямоугольным» истинно\n");
                else
                    printf("Высказывание «Треугольник со сторонами a, b, c является прямоугольным» ложно\n");
            break;
        case 7:
            printf("Выбрано 7 задание\n");
            int a7, b7, c7;
            printf("Введите сторону треугольника А: ");
            scanf("%d",&a7);
            printf("Введите сторону треугольника B: ");
            scanf("%d",&b7);
            printf("Введите сторону треугольника C: ");
            scanf("%d",&c7);
            if(a7+c7>b7 && a7+b7>c7 && b7+c7>a7)
                printf("Высказывание «Существует треугольник со сторонами a, b, c» истинно\n");
                else
                    printf("Высказывание «Существует треугольник со сторонами a, b, c» ложно\n");
            break;
    }
    return 0;
}
