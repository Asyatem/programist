#include <stdio.h>
#include<locale.h>
#include<math.h>
#include<stdlib.h>
int main(){
   int f;
    printf("Выберите одну из 6 программ: ");
    scanf("%d",&f);
    switch(f) {
        case 1:
            printf("Выбрано 1 задание\n");
            int a1, b1;
            printf("Введите значение переменной А: ");
            scanf("%d", &a1);
            printf("Введите значение переменной B: ");
            scanf("%d", &b1);
            if(a1==b1){
                a1=0;
                b1=0;
                printf("Новое значние переменной A: %d\nНовое значени переменной B: %d\n",a1, b1);
            } else {
                if(a1>b1){
                    b1=a1;
                    printf("Новое значние переменной A: %d\nНовое значени переменной B: %d\n",a1, b1);
                } else{
                    a1=b1;
                    printf("Новое значние переменной A: %d\nНовое значени переменной B: %d\n",a1, b1);
                }
            }
            break;
        case 2:
            printf("Выбрано 2 задание\n");
            int a2, b2, c2;
            printf("Введите значение переменной А: ");
            scanf("%d", &a2);
            printf("Введите значение переменной B: ");
            scanf("%d", &b2);
            printf("Введите значение переменной C: ");
            scanf("%d", &c2);
            if((a2>c2 && c2>b2) ||(c2>a2 && a2>b2)) {
                a2=a2+c2;
                printf("Сумма двух наибольших чисел: %d\n",a2);
            } else {
                if((a2>b2 && b2>c2) || (b2>a2 && a2>c2)) {
                    a2=a2+b2;
                    printf("Сумма двух наибольших чисел: %d\n",a2);
                } else {
                    b2=b2+c2;
                    printf("Сумма двух наибольших чисел: %d\n",b2);
                }
            }
            break;
        case 3:
            printf("Выбрано 3 задание\n");
            int a3, b3, c3;
            printf("Введите значение точки А: ");
            scanf("%d", &a3);
            printf("Введите значение точки B: ");
            scanf("%d", &b3);
            printf("Введите значение точки C: ");
            scanf("%d", &c3);
            b3=abs(a3-b3);
            c3=abs(a3-c3);
            if(b3>c3)
                printf("Точка  С находиться ближе ки точке А на расстоянии: %d\n", c3);
            else
                printf("Точка  B находиться ближе ки точке А на расстоянии: %d\n", b3);
            break;
        case 4:
            printf("Выбрано 4 задание\n");
            int x, y;
            printf(" Координата по Х: ");
            scanf("%d",&x);
            printf("Координата по У: ");
            scanf("%d",&y);
            if (x>0 && y>0)
                printf("Точка находиться в 1 четверти координатной плоскости.\n");
            else {
                if (x<0 && y>0)
                    printf("Точка находиться в 2 четверти координатной плоскости.\n");
                else {
                    if(x<0 && y<0)
                        printf("Точка находиться в 3 четверти координатной плоскости.\n");
                    else{
                        if(x>0 && y<0)
                            printf("Точка находиться в 4 четверти координатной плоскости.\n");
                        else
                            printf("Точка находиться на числовой оси.\n");
                    }
                }
            }
            break;
        case 5:
            printf("Выбрано 5 задание\n");
            int a6;
            printf("Введите число: ");
            scanf("%d",&a6);
            if(a6==0)
                printf("Описание числа: нулевое\n");
            else{
                if(a6>0 && a6%2==0)
                    printf("Описание числа: положительное, чётное\n");
                else{
                    if (a6<0 && a6%2==0)
                            printf("Описание числа: отрицательное, чётное\n");
                    else{
                        if(a6<0 && a6!=0)
                                printf("Описание числа: отрицательное, нечётное\n");
                        else
                        printf("Описание числа: положительное, нечётное\n");
                    }
                }
            }
            break;
        case 6:
            printf("Выбрано 6 задание\n");
            int a7;
            printf("Введите число: ");
            scanf("%d",&a7);
            if (a7>0 && a7<1000){
                if(a7/100>0) {
                    if(a7%2==0)
                        printf("Описание числа: трёхзначное, чётное\n");
                    else
                        printf("Описание числа: трёхзначное, нечётное\n");
                } else {
                    if (a7/10>0) {
                        if(a7%2==0)
                            printf("Описание числа:  двухзначное, чётное\n");
                        else
                            printf("Описание числа: двухзначное, нечётное\n");
                    } else {
                        if(a7%2==0)
                            printf("Описание числа:  однозначное, чётное\n");
                        else
                            printf("Описание числа: однозначное, нечётное\n");
                    }
                    
                }
            }else
                printf("Число не находиться в заданном промежутке\n");
            break;
    }
    return 0;
}
