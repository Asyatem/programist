#include <stdio.h>
#include<locale.h>
#include<math.h>
int main(){
   int f;
    printf("Выберите одну из 6 программ: ");
    scanf("%d",&f);
    switch(f) {
        case 1:
            printf("Выбрано 1 задание.Даны целые положительные числа A и B (A < B). Вывести все целые числа от A до B включительно; при этом каждое число должно выводиться столько раз, каково его значение.\n");
            int A,B,i;
            printf("Введите число А: ");
            scanf("%d",&A);
            printf("Введите число B: ");
            scanf("%d",&B);
            for(A; A<=B; A++){
                for(i=0; i<A; i++)
                    printf("%d ",A);
            }
        break;
        case 2:
            printf("Выбрано 2 задание.Даны положительные числа A и B (A > B). На отрезке длины A размещено максимально возможное количество отрезков длины B (без наложений). Не используя операции умножения и деления, найти длину незанятой части отрезка A.\n");
            int A1, B1;
            printf("Введите длину отрезка А: ");
            scanf("%d",&A1);
            printf("Введите длину отрезка B: ");
            scanf("%d",&B1);
            printf("Длина не занятой части отрезка А: %d\n", A1%B1);
            break;
        case 3:
            printf("Выбрано 3 задание.Дано целое число N (> 1). Вывести наименьшее из целых чисел K, для которых сумма 1 + 2 + . . . + K будет больше или равна N, и саму эту сумму.\n");
            int k=1, temp=1, N;
            printf("Введите число N: ");
            scanf("%d",&N);
               while (temp<N) {
                   k++;
                   temp += k;
               }
               printf("Число K: %d\nCумма чисел: %d\n",k,temp);
            break;
        case 4:
            printf("Выбрано 4 задание.Начальный вклад в банке равен 1000 руб. Через каждый месяц размер вклада увеличивается на P процентов от имеющейся суммы (P — вещественное число, 0 < P < 25). По данному P определить, через сколько месяцев размер вклада превысит 1100 руб., и вывести найденное количество месяцев K (целое число) и итоговый размер вклада S (вещественное число).\n");
            float P, V=1000; int M=0;
            printf("Введите размер процентов по вкладу Р: ");
            scanf("%f",&P);
               while(V<=1100){
                   M++;
                   V += V*P/100;
            }
            printf("Количество месяцев: %d\nИтоговы рзмер вклада: %f",M, V);
            break;
        case 5:
            printf("Выбрано 5 задание.Даны целые положительные числа A и B. Найти их наибольший общий делитель (НОД), используя алгоритм Евклида.\n");
            int A5, B5;
            printf("Введитеи число A:" );
            scanf("%d", &A5);
            printf("Введитеи число B:" );
            scanf("%d", &B5);
            while (A5 != 0 && B5 != 0){
                if (A5 > B5)
                    A5 = A5 % B5;
                else
                    B5 = B5 % A5;
                    }
            printf("Наибольший общий делитель А и В: %d\n", A5+B5);
            break;
        case 6:
            printf("Выбрано 6 задание.Дано целое число N (> 1), являющееся числом Фибоначчи: N = FK. Найти целое число K — порядковый номер числа Фибоначчи N.\n");
            int f=0, f1=1, f2=2, n, k6=2;
            printf("Введите число Фибоначи N: ");
            scanf("%d",&n);
            while(f<=n){
                k6++;
                f=f1+f2;
                f2=f1;
                f1=f;
            }
            printf("Порядковый номер числа Фибоначи: %d\n", k6);
            break;
    }
    return 0;
}
